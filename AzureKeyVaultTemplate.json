{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
	"factoryName": {
      "type": "string",
      "metadata": "Data Factory name"
    },
        "AzureKeyVaultTemplate_properties_typeProperties_baseUrl": {
		  "type": "string",
		  "defaultValue": "https://Sanagam-KeyVaut-dev.vault.azure.net/"
		}
    },
	
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.DataFactory/factories/linkedServices",
			"name": "[concat(parameters('factoryName'), '/AzureKeyVaultTemplate')]"
            "apiVersion": "2018-06-01",
            "location": "eastus",
            "properties": {
				"annotations": [
				  
				],
				"type": "AzureKeyVault",
				"typeProperties": {
				  "baseUrl": "[parameters('AzureKeyVaultTemplate_properties_typeProperties_baseUrl')]"
				}
			},
            "dependsOn": [
				"[parameters('factoryName')]"
			]
        }
    ]
}